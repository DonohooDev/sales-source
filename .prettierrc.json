{
    "bracketSpacing": true,
    "proseWrap": "never",
    "singleQuote": false,
    "semi": true,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 100,
    "trailingComma": "es5",
    "arrowParens": "always",
    "endOfLine": "lf",
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 160,
          "tabWidth": 2
        }
      }
    ]
  }
  